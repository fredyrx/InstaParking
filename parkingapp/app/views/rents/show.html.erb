    <h2>Alquiler de estacionamiento</h2>
<div class="row">
<div class="col-md-4">

<p>
    <strong>Id:</strong> <%= @rent.id %>
</p>
<p>
    <strong>Estacionamiento:</strong> <%= @rent.parking_lot.name %>
</p>
<p>
    <strong>Fecha:</strong> <%= @rent.date %>
</p>
<p>
    <strong>Precio:</strong> S/. <%= @rent.total_price %>
</p>
<p>
    <strong>Horario:</strong> <%= @rent.start_time.strftime('%r') %> - <%= @rent.end_time.strftime('%r') %>
</p>
<p>
    <strong>Tiempo:</strong> <%= @rent.total_minutes %>
</p>